import java.util.Locale

def uuid = java.util.UUID.randomUUID().toString()
def price = String.format(Locale.US, &quot;%.2f&quot;, (Math.random() * 400) + 10)
def stock = (int)(Math.random() * 400)

def base = &quot;&quot;&quot;{&quot;sku&quot;:&quot;UPD-${uuid.substring(0,12)}&quot;,&quot;name&quot;:&quot;Updated-${uuid.substring(0,6)}&quot;,&quot;price&quot;:${price},&quot;stock&quot;:${stock},&quot;categoryId&quot;:${categoryId},&quot;details&quot;:&quot;&quot;}&quot;&quot;&quot;
int targetBytes = Integer.parseInt(vars.get(&quot;payloadSize&quot;))
int currentBytes = base.getBytes(&quot;UTF-8&quot;).length
int fillerBytes = Math.max(0, targetBytes - currentBytes)
def filler = (fillerBytes &gt; 0) ? RandomStringUtils.randomAlphanumeric(fillerBytes) : &quot;&quot;
def payload = base.replace(&quot;\&quot;\&quot;&quot;, &quot;\&quot;${filler}\&quot;&quot;)

vars.put(&quot;itemBodyLarge&quot;, payload)
</stringProp>
